<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml" fx:controller="com.projeto.lanhouse.lanhousejavafx.controller.AdicionarJogoController">
    <children>
      <AnchorPane prefHeight="269.0" prefWidth="400.0">
         <children>
              <Label layoutX="135.0" layoutY="20.0" style="-fx-font-size: 16px;" text="Adicione um jogo!" />
              <TextField fx:id="nomeField" layoutX="126.0" layoutY="58.0" promptText="Nome do Jogo" />
              <TextField fx:id="generoField" layoutX="126.0" layoutY="97.0" promptText="Gênero" />
              <TextField fx:id="desenvolvedorField" layoutX="126.0" layoutY="139.0" promptText="Desenvolvedor" />
              <TextField fx:id="classificacaoEtariaField" layoutX="126.0" layoutY="181.0" promptText="Classificação Etária" />
              <Button layoutX="167.0" layoutY="221.0" onAction="#adicionarJogo" text="Adicionar" />
         </children>
      </AnchorPane>
    </children>
    <VBox.margin>
        <Insets bottom="20" left="20" right="20" top="20" />
    </VBox.margin>
</VBox>
